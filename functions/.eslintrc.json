{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "plugins": [
    "promise"
  ],
  "extends": "airbnb",
  "rules": {
    "no-use-before-define": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": "off",
    "linebreak-style": "off",
    "indent": ["error", 4]
  }
}
